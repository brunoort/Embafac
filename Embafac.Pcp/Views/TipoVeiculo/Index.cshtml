@model IEnumerable<Embafac.Pcp.Entidades.Models.TipoVeiculo>

@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Tipo de Veículos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descricao)
        </th>
        <th></th>
    </tr>


@foreach (var item in ViewBag.TipoVeiculo) { 
    <tr>
        <td>
            @item.Id
        </td>
        <td>
            @item.Descricao
        </td>
       <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Remover", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
